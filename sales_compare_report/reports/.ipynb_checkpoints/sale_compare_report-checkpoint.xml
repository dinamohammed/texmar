<odoo>
    <template id="report_sales_compare_document">
        <t t-call="web.html_container">
        <t t-call="web.external_layout">
            <div class="page"  style="font-size:15px;">
                <h3><strong>PREPARATION</strong></h3>
                <table class="table" style="width:100%;">
                            <tr>
                                <th class="text-right"><span>Ser.</span></th>
                                <th class="text-right"><span>Code</span></th>
                                <th class="text-right"><span>Name</span></th>
                                <th class="text-right"><span>Color</span></th>
                                <th class="text-right"><span>Deliver Date</span></th>
                                <th class="text-right"><span>Qty</span></th>
                                <th class="text-center"><span>PO</span></th>
                                <th class="text-center"><span>Current Year</span></th>
                                <th class="text-center"><span>Last Years</span></th>
                                <th class="text-center"><span>Total</span></th>
                                <th class="text-center"><span>Request</span></th>
                                <th class="text-center"><span>PO Qty</span></th>
                            </tr>
                            <tr>
                                <th class="text-right"><span></span></th>
                                <th class="text-right"><span></span></th>
                                <th class="text-right"><span></span></th>
                                <th class="text-right"><span></span></th>
                                <th class="text-right"><span></span></th>
                                <th class="text-right"><span></span></th>
                                <th class="text-center">
                                    <span>PO </span>
                                    <span>| </span>
                                    <span>QTY </span>
                                    <span>| </span>
                                    <span>Deliver </span>
                                    <span>| </span>
                                    <span>Rest </span>
                                    <span>| </span>
                                    <span>Date </span>
                                    <span>| </span>
                                    <span>Status </span>
                                </th>
                                <th class="text-center">
                                    <span>Gallery </span>
                                    <span>| </span>
                                    <span>Dealer </span>
                                    <span>| </span>
                                    <span>Market </span>
                                </th>
                                <th class="text-center">
                                    <span>Gallery </span>
                                    <span>| </span>
                                    <span>Dealer </span>
                                    <span>| </span>
                                    <span>Market </span>
                                </th>
                                <th class="text-center"><span></span></th>
                                <th class="text-center"><span></span></th>
                                <th class="text-center"><span></span></th>
                            </tr>
                    <t t-set="serial" t-value="1"/>
                    <t t-set="product_id" t-value="0"/>
                        <t t-foreach="purchase_lines" t-as="line">
                            <tr>
                                <t t-if = 'line.product_id.id != product_id'>
                                    <td class="text-right"><span t-esc="serial"/></td>
                                    <td class="text-right"><span t-esc="line.product_id.default_code"/></td>
                                    <td class="text-right"><span t-esc="line.product_id.name"/></td>
                                    <td class="text-right"><span t-esc="line.product_id.product_color"/></td>
                                    <td class="text-right">nullll</td>
                                    <td class="text-right"><span t-esc="line.product_id.qty_available"/></td>
                                </t>
                                <t t-else = ''>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                </t>
                                <td class="text-center">
                                    <span t-esc="line.order_id.name"/>
                                    <span> | </span>
                                    <span t-esc="line.product_qty"/>
                                    <span> | </span>
                                    <span t-esc="line.qty_received"/>
                                    <span> | </span>
                                    <span t-esc="line.remaining_qty"/>
                                    <span> | </span>
                                    <span t-esc="line.date_planned"/>
                                    <span> | </span>
                                    <span t-esc="line.state"/>
                                </td>
                                <t t-if = 'line.product_id.id != product_id'>
                                    <td class="text-center">
                                        <span t-esc="line.product_id.total_market"/>
                                        <span> | </span>
                                        <span t-esc="line.product_id.total_gallery"/>
                                        <span> | </span>
                                        <span t-esc="line.product_id.total_dealer"/>
                                    </td>
                                    <td class="text-center">
                                        <span t-esc="line.product_id.last_total_market"/>
                                        <span> | </span>
                                        <span t-esc="line.product_id.last_total_gallery"/>
                                        <span> | </span>
                                        <span t-esc="line.product_id.last_total_dealer"/>
                                    </td>
                                </t>
                                <t t-else = ''>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                </t>
                            </tr>
                            <t t-set="product_id" t-value="line.product_id.id"/>
                            <t t-set="serial" t-value="serial + 1"/>
                        </t>
                </table>
            </div>
            </t>
        </t>
    </template>
    
